{
	"env": {
			"es2021": true,
			"node": true
	},
	"extends": [
			"airbnb-base",
			"prettier"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
	},
	"plugins": [
			"@typescript-eslint",
			"prettier"
	],
	"rules": {
			"prettier/prettier": "error",
			"no-console": 0,
			"import/no-unresolved": "error",
			"import/extensions": [
				"error",
				"ignorePackages",
				{
					"js": "never",
					"jsx": "never",
					"ts": "never",
					"tsx": "never"
				}
		 ],
		 "import/no-cycle": 0,
		 "import/no-named-as-default": 0,
		 "no-throw-literal": 0,
		 "no-use-before-define": 0
	},
"settings": {
	"import/parsers": {
		"@typescript-eslint/parser": [".ts", ".tsx"]
	},
	"import/resolver": {
		"node": {
			"extensions": [".js", ".jsx", ".ts", ".tsx"]
		}
	}
	}
}
